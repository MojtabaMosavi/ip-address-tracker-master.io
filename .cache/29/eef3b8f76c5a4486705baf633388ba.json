\n@import url(\"https://fonts.googleapis.com/css2?family=Rubik:wght@400;500;700&display=swap\");\r\n$fontsize-textinput: 18px;\r\n$font-primary: \"Rubik\", sans-serif;\r\n\r\n/*|| breakpoints */\r\n$breakpoints: (\r\n  \"tablet\": 43em,\r\n  \"laptop\": 62em,\r\n  \"desktop\": 82em,\r\n);\r\n","scss/abstracts/_mixins.scss":"@use \"./variables\" as v;\r\n\r\n/*----------------------*\\\r\n    #Mixins \r\n\\*----------------------*/\r\n\r\n/*|| to deffine */\r\n@mixin respond-to($breakpoint) {\r\n  @media screen and (min-width: map-get($map: v.$breakpoints, $key: $breakpoint)) {\r\n    @content;\r\n  }\r\n}\r\n\r\n/*|| to make a flex container */\r\n@mixin flex-container(\r\n  $al-con: \"center\",\r\n  $js-con: \"space-between\",\r\n  $flex-d: \"row\"\r\n) {\r\n  display: flex;\r\n  align-items: $al-con;\r\n  justify-content: $js-con;\r\n  flex-direction: $flex-d;\r\n} ;\r\n","scss/base/_helpers.scss":"@use \"../abstracts\" as abs;\r\n\r\n/*----------------------*\\\r\n    #Helper classes\r\n\\*----------------------*/\r\n\r\n/*|| container to align the content*/\r\n\r\n.container {\r\n  padding: 0 1.4rem;\r\n}\r\n\r\n/*|| displays the content for screen readers */\r\n\r\n.sr-only {\r\n  position: absolute;\r\n  left: -10000px;\r\n  height: auto;\r\n  width: 1px;\r\n  height: 1px;\r\n  overflow-x: hidden;\r\n}\r\n","scss/base/_typography.scss":"/*-------------------*\\\r\n    #Typography\r\n\\*-------------------*/\r\n\r\n/*|| general reset*/\r\nhtml {\r\n  font-size: 100%;\r\n}\r\n","scss/base/_resets.scss":"@use \"../abstracts\" as abs;\r\n\r\n/*-------------------*\\\r\n    #Resets\r\n\\*-------------------*/\r\n\r\n/*||  general resets */\r\n\r\nhtml {\r\n  box-sizing: border-box;\r\n}\r\n\r\n*,\r\n*::before,\r\n*::after {\r\n  box-sizing: inherit;\r\n  margin: 0;\r\n  padding: 0;\r\n}\r\n\r\nbody {\r\n  min-height: 100vh;\r\n  overflow-x: hidden;\r\n  font-family: abs.$font-primary;\r\n}\r\n\r\n/*|| making the images and picture sizes limited to the containing element*/\r\nimage,\r\npicture {\r\n  max-width: 100%;\r\n}\r\n\r\n/*|| remove all the default styles from list */\r\nul,\r\nol {\r\n  list-style: none;\r\n}\r\n\r\n/*|| remove default style from links*/\r\na,\r\na:visited {\r\n  text-decoration: none;\r\n}\r\n\r\n/*|| remove default style from buttons*/\r\nbutton {\r\n  border: none;\r\n}\r\n\r\n/*|| remove default style from links and buttons \r\n      upon hovering but not when they are tabed*/\r\n\r\nbutton,\r\na {\r\n  cursor: pointer;\r\n  &:focus {\r\n    outline: max(1, 0.1em) dashed #000;\r\n    outline-offset: -0.25em;\r\n  }\r\n\r\n  &:focus:not(:focus-visible) {\r\n    outline: none;\r\n  }\r\n\r\n  &:focus-visible {\r\n    outline: max(1, 0.1em) dashed #000;\r\n    outline-offset: -0.25em;\r\n  }\r\n}\r\n\r\n/*|| Remove all animations and transitions\r\nfor people that prefer not to see them */\r\n@media (prefers-reduced-motion: reduce) {\r\n  *,\r\n  *::before,\r\n  *::after {\r\n    animation-duration: 0.01ms !important;\r\n    animation-iteration-count: 1 !important;\r\n    transition-duration: 0.01ms !important;\r\n    scroll-behavior: auto !important;\r\n  }\r\n}\r\n","scss/components/_ip-tracker.scss":"@use \"../abstracts\" as abs;\r\n\r\n/*--------------------*\\\r\n  #ip-tracker\r\n\\*--------------------*/\r\n\r\n.ip-tracker {\r\n  &__header {\r\n    background: url(\"../assets/images/pattern-bg.png\") no-repeat center center/\r\n      cover;\r\n    text-align: center;\r\n  }\r\n\r\n  &-wrapper {\r\n    padding-top: 1.7rem;\r\n    margin-bottom: 2rem;\r\n  }\r\n\r\n  &__heading {\r\n    font-size: 1.5rem;\r\n    margin-bottom: 1.85rem;\r\n    color: var(--headings);\r\n  }\r\n\r\n  &__form {\r\n    display: inline-block;\r\n    width: 100%;\r\n    position: relative;\r\n\r\n    &::after {\r\n      content: \"\";\r\n      background: url(../assets/images/icon-arrow.svg) no-repeat center,\r\n        var(--black);\r\n      position: absolute;\r\n      width: 3.7rem;\r\n      right: 0;\r\n      top: 0;\r\n      height: 100%;\r\n      border-top-right-radius: 1rem;\r\n      border-bottom-right-radius: 1rem;\r\n    }\r\n  }\r\n\r\n  &__input {\r\n    text-align: left;\r\n    padding: 1.2rem 1.5rem;\r\n    width: 100%;\r\n    font-size: 1.1rem;\r\n    border-radius: 1rem;\r\n    border: none;\r\n  }\r\n\r\n  &__info {\r\n    display: none;\r\n  }\r\n}\r\n","scss/main.scss":"/*-------------------------------*\\\r\n  #Main stylesheet\r\n\\*-------------------------------*/\r\n\r\n/*\r\n* import all the other sass modules \r\n*/\r\n\r\n@use \"./abstracts\";\r\n@use \"./base\";\r\n@use \"./components\";\r\n@use \"./layout\";\r\n"},"lineCount":null}},"error":null,"hash":"506e307280440f24ae878cc6e0b20793","cacheData":{"env":{}}}